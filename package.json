    "immutability-helper": "^2.8.1",
    "lodash": "^4.17.11",
    "md5": "^2.2.1",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dates": "^18.1.0",
    "react-dom": "^16.5.2",
    "react-emotion": "^9.2.10",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.6.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "next",
    "recompose": "^0.30.0",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-persist": "^5.10.0",
    "slugify": "^1.3.1",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "prettier-watch": "onchange '**/*.js' -- prettier --write {{changed}}",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "i18n:extract": "react-intl-cra 'src/**/*.{js,jsx}' -o 'src/i18n/i18n.json'",
    "i18n:translate": "node ./i18n.js",
    "i18n": "yarn run i18n:extract && yarn run i18n:translate",
    "develop": "concurrently \"yarn run start\" \"yarn run prettier-watch\""
  },
  "devDependencies": {
    "babel-plugin-emotion": "^9.2.10",
    "babel-plugin-macros": "^2.4.2",
    "concurrently": "^4.0.1",
    "onchange": "^4.1.0",
    "prettier": "^1.14.3",
    "react-intl-cra": "^0.3.3",
    "react-intl-translations-manager": "^5.0.3"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">0.25%",
      "not op_mini all",
      "ie 11"
    ]
  }
